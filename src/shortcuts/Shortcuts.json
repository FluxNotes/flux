[ { "type": "InsertValue",
    "name": "MrnInserter",
    "getData": [ {"object": "patient", "method": "getMRN"}],
    "isContext": false,
    "stringTriggers": [{"name":"mrn", "description": "Medical Reference Number for patient"}],
    "knownParentContexts": "Patient"
  },
  { "type": "InsertValue",
    "name": "AgeInserter",
