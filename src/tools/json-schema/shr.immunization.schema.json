{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://jstars-linux-1.mitre.org/json-schema/shr/immunization",
  "title": "TODO: Figure out what the title should be.",
  "definitions": {
    "Immunization": {
      "allOf": [
        {
          "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/base#/definitions/Entry"
        },
        {
          "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/base#/definitions/Action"
        },
        {
          "type": "object",
          "properties": {
            "Vaccine": {
              "$ref": "#/definitions/Vaccine"
            },
            "NonOccurrenceModifier": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/base#/definitions/NonOccurrenceModifier"
            },
            "BodySite": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/core#/definitions/BodySite"
            },
            "RouteIntoBody": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/medication#/definitions/RouteIntoBody"
