            "AmountOfMedication": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/medication#/definitions/AmountOfMedication"
            }
          },
          "required": [
            "Vaccine",
            "NonOccurrenceModifier"
          ]
        }
      ],
      "description": "A statement relating to a vaccination that was or was not administered to the subject.\nConcepts: http://ncimeta.nci.nih.gov:C0020971\nTBD Fields: DoseSequenceNumber"
    },
    "ImmunizationAdministered": {
      "allOf": [
        {
          "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/base#/definitions/Entry"
        },
        {
          "$ref": "#/definitions/Immunization"
        },
        {
          "type": "object",
          "properties": {
            "Vaccine": {},
            "NonOccurrenceModifier": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/base#/definitions/NonOccurrenceModifier"
