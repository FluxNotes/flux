    },
    "ImmunizationNotGiven": {
      "allOf": [
        {
          "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/base#/definitions/Entry"
        },
        {
          "$ref": "#/definitions/Immunization"
        },
        {
          "type": "object",
          "properties": {
            "NonOccurrenceModifier": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/base#/definitions/NonOccurrenceModifier"
            }
          }
        }
      ],
      "description": "Represents an immunization that was not given.\nConcepts: http://ncimeta.nci.nih.gov:C0580520\nTBD Fields: DoseSequenceNumber"
    },
    "LotNumber": {
      "type": "object",
      "properties": {
        "Value": {
          "type": "string"
        }
      },
      "description": "A distinctive alpha-numeric identification code assigned by the manufacturer or distributor to a specific quantity of manufactured material or product within a batch.\nConcepts: http://ncimeta.nci.nih.gov:C1115660",
      "required": [
        "Value"
      ]
    },
    "Vaccine": {
      "type": "object",
      "properties": {
        "Value": {
          "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/core#/definitions/SpecificType"
        },
        "VaccineManufacturer": {
          "$ref": "#/definitions/VaccineManufacturer"
        },
        "LotNumber": {
          "$ref": "#/definitions/LotNumber"
        }
      },
      "description": "A specific type or manufactured instance of a vaccine, a prophylactic or therapeutic preparation given to produce immune response to pathogenic organisms or substances.\nConcepts: http://ncimeta.nci.nih.gov:C1543322",
      "required": [
        "Value"
      ]
    },
    "VaccineManufacturer": {
      "type": "object",
      "properties": {
        "Value": {
          "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/actor#/definitions/Organization"
        }
      },
      "description": "The organization that produces the vaccine.\nConcepts: http://ncimeta.nci.nih.gov:C1114744",
      "required": [
        "Value"
      ]
    }
