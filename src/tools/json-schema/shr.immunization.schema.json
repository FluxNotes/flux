            },
            "RouteIntoBody": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/medication#/definitions/RouteIntoBody"
            },
            "AmountOfMedication": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/medication#/definitions/AmountOfMedication"
            }
          }
        }
