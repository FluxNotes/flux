{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://jstars-linux-1.mitre.org/json-schema/shr/vital",
  "title": "TODO: Figure out what the title should be.",
  "definitions": {
    "BloodPressure": {
      "allOf": [
        {
          "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/base#/definitions/Entry"
        },
        {
          "$ref": "#/definitions/VitalSign"
        },
        {
          "type": "object",
          "properties": {
            "BodySite": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/core#/definitions/BodySite"
            },
            "TestMethod": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/lab#/definitions/TestMethod"
            },
            "ObservationQualifier": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/observation#/definitions/ObservationQualifier"
            },
            "SystolicPressure": {
              "$ref": "#/definitions/SystolicPressure"
            },
            "DiastolicPressure": {
              "$ref": "#/definitions/DiastolicPressure"
            },
            "HeadTiltAngle": {
              "$ref": "#/definitions/HeadTiltAngle"
            }
          },
          "required": [
            "SystolicPressure",
            "DiastolicPressure"
          ]
        }
      ],
      "description": "The force of circulating blood on the walls of the arteries.\nConcepts: http://loinc.org:55284-4"
    },
    "BodyHeight": {
      "allOf": [
        {
          "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/base#/definitions/Entry"
        },
        {
          "$ref": "#/definitions/VitalSign"
        },
        {
          "type": "object",
          "properties": {
            "Value": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/core#/definitions/Quantity"
            },
            "ObservationQualifier": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/observation#/definitions/ObservationQualifier"
            }
          },
          "required": [
            "Value"
          ]
        }
      ],
      "description": "The distance from the sole to the crown of the head.\nConcepts: http://loinc.org:8302-2"
    },
    "BodyLength": {
      "allOf": [
        {
          "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/base#/definitions/Entry"
        },
        {
          "$ref": "#/definitions/BodyHeight"
        },
        {
          "type": "object",
          "properties": {
            "Value": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/core#/definitions/Quantity"
            }
          },
          "required": [
            "Value"
          ]
        }
      ],
      "description": "The distance from the sole of the foot to the crown of the head, lying down (typically 0-2 years).\nConcepts: http://loinc.org:8306-3"
    },
    "BodyMassIndex": {
      "allOf": [
        {
          "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/base#/definitions/Entry"
        },
        {
          "$ref": "#/definitions/VitalSign"
        },
        {
          "type": "object",
          "properties": {
            "Value": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/core#/definitions/Quantity"
            },
            "BodyWeight": {
              "type": "object",
              "properties": {
                "ShrId": {
                  "type": "string"
                },
                "EntryType": {
                  "type": "string",
                  "enum": [
                    "http://jstars-linux-1.mitre.org/json-schema/shr/vital#/definitions/BodyWeight"
                  ]
                },
                "EntryId": {
                  "type": "string"
                }
              },
              "required": [
                "ShrId",
                "EntryType",
                "EntryId"
              ]
            },
            "BodyHeight": {
              "type": "object",
              "properties": {
                "ShrId": {
                  "type": "string"
                },
                "EntryType": {
                  "type": "string",
                  "enum": [
                    "http://jstars-linux-1.mitre.org/json-schema/shr/vital#/definitions/BodyHeight"
                  ]
                },
                "EntryId": {
                  "type": "string"
                }
              },
              "required": [
                "ShrId",
                "EntryType",
                "EntryId"
              ]
            }
          },
          "required": [
            "Value"
          ]
        }
      ],
      "description": "A general indicator of the body fat an individual is carrying based upon the ratio of weight to height.\nConcepts: http://loinc.org:39156-5"
    },
    "BodyTemperature": {
      "allOf": [
        {
          "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/base#/definitions/Entry"
        },
        {
          "$ref": "#/definitions/VitalSign"
        },
        {
          "type": "object",
          "properties": {
            "Value": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/core#/definitions/Quantity"
            },
            "BodySite": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/core#/definitions/BodySite"
            },
            "ObservationQualifier": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/observation#/definitions/ObservationQualifier"
            }
          },
          "required": [
            "Value"
          ]
        }
      ],
      "description": "The measure of the level of heat in a human or animal.\nConcepts: http://loinc.org:8310-5"
    },
    "BodyWeight": {
      "allOf": [
        {
          "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/base#/definitions/Entry"
        },
        {
          "$ref": "#/definitions/VitalSign"
        },
        {
          "type": "object",
          "properties": {
            "Value": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/core#/definitions/Quantity"
            },
            "ObservationQualifier": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/observation#/definitions/ObservationQualifier"
            }
          },
          "required": [
            "Value"
          ]
        }
      ],
      "description": "The mass or quantity of heaviness of an individual. Does not include estimates of fetal bodyweight.\nConcepts: http://loinc.org:29463-7"
    },
    "DiastolicPressure": {
      "allOf": [
        {
          "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/observation#/definitions/ObservationComponent"
        },
        {
          "type": "object",
          "properties": {
            "Value": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/core#/definitions/Quantity"
            },
            "SpecificType": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/core#/definitions/SpecificType"
            }
          },
          "required": [
            "Value"
          ]
        }
      ],
      "description": "The blood pressure after the contraction of the heart while the chambers of the heart refill with blood, when the pressure is lowest.\nConcepts: http://loinc.org:8462-4"
    },
    "HeadCircumference": {
      "allOf": [
        {
          "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/base#/definitions/Entry"
        },
        {
          "$ref": "#/definitions/VitalSign"
        },
        {
          "type": "object",
          "properties": {
            "Value": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/core#/definitions/Quantity"
            }
          },
          "required": [
            "Value"
          ]
        }
      ],
      "description": "Circumference of the head (typically 0-2 years).\nConcepts: http://loinc.org:8287-5"
    },
    "HeadTiltAngle": {
      "type": "object",
      "properties": {
        "Value": {
          "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/core#/definitions/Quantity"
        }
      },
      "description": "The angle at which the head is tilted relative to a level position while physiologic tests are taken.\nConcepts: http://loinc.org:8360-0",
      "required": [
        "Value"
      ]
    },
    "HeartRate": {
      "allOf": [
        {
          "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/base#/definitions/Entry"
        },
        {
          "$ref": "#/definitions/VitalSign"
        },
        {
          "type": "object",
          "properties": {
            "Value": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/core#/definitions/Quantity"
            },
            "ObservationQualifier": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/observation#/definitions/ObservationQualifier"
            },
            "TestMethod": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/lab#/definitions/TestMethod"
            }
          },
          "required": [
            "Value"
          ]
        }
      ],
      "description": "The number of times the heart ventricles contract per unit of time, usually per minute.\nConcepts: http://loinc.org:8867-4"
    },
    "OxygenSaturation": {
      "allOf": [
        {
          "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/base#/definitions/Entry"
        },
        {
          "$ref": "#/definitions/VitalSign"
        },
        {
          "type": "object",
          "properties": {
            "Value": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/core#/definitions/Quantity"
            },
            "BodySite": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/core#/definitions/BodySite"
            }
          },
          "required": [
            "Value"
          ]
        }
      ],
      "description": "Oxygen saturation in Arterial blood by Pulse oximetry.\nConcepts: http://loinc.org:59408-5"
    },
    "RespiratoryRate": {
      "allOf": [
        {
          "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/base#/definitions/Entry"
        },
        {
          "$ref": "#/definitions/VitalSign"
        },
        {
          "type": "object",
          "properties": {
            "Value": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/core#/definitions/Quantity"
            },
            "ObservationQualifier": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/observation#/definitions/ObservationQualifier"
            },
            "TestMethod": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/lab#/definitions/TestMethod"
            }
          },
          "required": [
            "Value"
          ]
        }
      ],
      "description": "The rate of breathing (inhalation and exhalation) measured within in a unit time, expressed as breaths per minute.\nConcepts: http://loinc.org:9279-1"
    },
    "SystolicPressure": {
      "allOf": [
        {
          "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/observation#/definitions/ObservationComponent"
        },
        {
          "type": "object",
          "properties": {
            "Value": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/core#/definitions/Quantity"
            },
            "SpecificType": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/core#/definitions/SpecificType"
            }
          },
          "required": [
            "Value"
          ]
        }
      ],
      "description": "The blood pressure during the contraction of the left ventricle of the heart, when blood pressure is at its highest.\nConcepts: http://loinc.org:8480-6"
    },
    "VitalSign": {
      "allOf": [
        {
          "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/lab#/definitions/Test"
        },
        {
          "type": "object",
          "properties": {
            "Value": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/core#/definitions/Quantity"
            },
            "Category": {
              "$ref": "http://jstars-linux-1.mitre.org/json-schema/shr/base#/definitions/Category"
            }
          },
          "required": [
            "Value"
          ]
        }
      ],
      "description": "Concepts: Vital Sign (http://loinc.org:8716-3)"
    }
  },
  "description": "The SHR Vital domain contains definitions for basic physiological measurements categorized as vital signs.",
  "type": "object",
  "anyOf": [
    {
      "$ref": "#/definitions/BloodPressure"
    },
    {
      "$ref": "#/definitions/BodyHeight"
    },
    {
      "$ref": "#/definitions/BodyLength"
    },
    {
      "$ref": "#/definitions/BodyMassIndex"
    },
    {
      "$ref": "#/definitions/BodyTemperature"
    },
    {
      "$ref": "#/definitions/BodyWeight"
    },
    {
      "$ref": "#/definitions/HeadCircumference"
    },
    {
      "$ref": "#/definitions/HeartRate"
    },
    {
      "$ref": "#/definitions/OxygenSaturation"
    },
    {
      "$ref": "#/definitions/RespiratoryRate"
    }
  ]
}